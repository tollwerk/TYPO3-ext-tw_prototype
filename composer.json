{
  "name": "tollwerk/tw_prototype",
  "type": "typo3-cms-extension",
  "description": "Prototypical TYPO3 main extension for Tollwerk projects",
  "homepage": "https://github.com/tollwerk/TYPO3-ext-tw_prototype",
  "license": "GPL-2.0+",
  "version": "1.0",
  "require": {
    "typo3/cms-core": "~7.6"
  },
  "autoload": {
    "psr-4": {
      "Tollwerk\\TwPrototype\\": "Classes/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tollwerk\\TwPrototype\\Tests\\": "Tests/",
      "TYPO3\\CMS\\Core\\Tests\\": ".Build/vendor/typo3/cms/typo3/sysext/core/Tests/"
    }
  },
  "replace": {
    "tw_prototype": "self.version",
    "typo3-ter/tw_prototype": "self.version"
  },
  "authors": [
    {
      "name": "Joschi Kuphal",
      "email": "joschi@tollwerk.de"
    }
  ],
  "extra": {
    "branch-alias": {
      "dev-master": "master"
    },
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": ".Build/Web"
    }
  }
}
